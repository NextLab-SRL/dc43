{
  "version": "0.1.0",
  "kind": "DataContract",
  "apiVersion": "3.0.2",
  "id": "demo.streaming.events_rejects",
  "name": "Streaming events (rejects)",
  "description": {"usage": "Rows rejected by the streaming validation scenarios."},
  "status": "active",
  "servers": [
    {
      "server": "local",
      "type": "filesystem",
      "path": "data/demo.streaming.events_rejects",
      "format": "parquet",
      "customProperties": [
        {
          "property": "dc43.core.versioning",
          "value": {
            "mode": "append",
            "includePriorVersions": true,
            "subfolder": "{version}",
            "filePattern": "part-*.parquet"
          }
        }
      ]
    }
  ],
  "schema": [
    {
      "name": "events",
      "properties": [
        {"name": "timestamp", "physicalType": "timestamp", "required": true},
        {"name": "value", "physicalType": "bigint", "required": true},
        {
          "name": "reject_reason",
          "physicalType": "string",
          "required": true
        }
      ]
    }
  ]
}
